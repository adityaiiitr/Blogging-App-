> Props destructuring

    function AdminLayout(props){
        return (

            <Layout>

                <Sider>SideBar</Sider>
                <Layout>
                <Content>{props.children}</Content>
                </Layout>
            </Layout>

        );
    }

BETTER WAY AHEAD
function AdminLayout({children}){
return (

            <Layout>

                <Sider>SideBar</Sider>
                <Layout>
                <Content>{children}</Content>
                </Layout>
            </Layout>

        );
    }

